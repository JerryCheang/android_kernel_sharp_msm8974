/include/ "msm8974-leds.dtsi"
/include/ "sharp-pa23-shdisp.dtsi"

&soc {
    /*---------------------*/
    /* BLSP1 QUP0(BLSP#1)  */
    /*---------------------*/
    blsp1_spi: spi@f9923000 {
        status = "ok";
        spi-max-frequency = <50000000>;
        qcom,gpio-cs0 = <&msmgpio 2 0>;
        qcom,use-bam;
    };

};
